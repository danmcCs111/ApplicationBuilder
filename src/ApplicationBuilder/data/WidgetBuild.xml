<Build>	
	<JFrame setLayout="BorderLayout" setTitle="Video Launcher" setSize="600, 250" setLocation="150,150">
		<JPanel setLayout="BorderLayout" extendedLayoutApplyParent="Center">
			<JScrollPane extendedLayoutApplyParent="North">
				<JPanel extendedLayoutApplyParent="">
					<SwappableCollection extendedArrayProcessingPath=".\src\ApplicationBuilder\Roku Channels@.url; .\src\ApplicationBuilder\Youtube Channels@.url">
						<JLabel setName="title" extendedLayoutApplyParent="" setForeground="100,50,150" setBackground="190,190,190">
						</JLabel>
						<JButtonArray setName="channels">
						</JButtonArray>
						<JButton setText="Close Video">
						</JButton>
					</SwappableCollection>
				</JPanel>
			</JScrollPane>
		</JPanel>
		<JButton setText="&lt;" extendedLayoutApplyParent="West" addActionListener="NavigationButtonActionListener" extendedActionListenerSubType="WidgetExtensions.Direction@BACKWARD" extendedActionListenerConnectedComponent="WidgetExtensions.SwappableCollection">
		</JButton>
		<JButton setText="&gt;" extendedLayoutApplyParent="East" addActionListener="NavigationButtonActionListener" extendedActionListenerSubType="WidgetExtensions.Direction@FORWARD" extendedActionListenerConnectedComponent="WidgetExtensions.SwappableCollection">
		</JButton>
		
		<MenuOption setText="File">
			<JMenuItem setText="Reload" extendedLayoutApplyParent="" addActionListener="ReloadActionListener">
			</JMenuItem >
			<JMenuItem setText="MinimizeToSystemTray" extendedLayoutApplyParent="" addActionListener="MinimizeActionListener">
			</JMenuItem >
			<JMenuItem setText="Exit" extendedLayoutApplyParent="" addActionListener="ExitActionListener">
			</JMenuItem >
		</MenuOption>
		
		<SystemTray setText="Video Launcher">
			<JMenuItem setText="Open" >
			</JMenuItem>
			<JMenuItem setText="Close" >
			</JMenuItem>
		</SystemTray>
	</JFrame>
</Build>
