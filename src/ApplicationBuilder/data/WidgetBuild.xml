<Build>	
	<JFrame setLayout="BorderLayout" setTitle="Video Launcher" setSize="600, 250" setLocation="150,150">
		<JPanel setLayout="BorderLayout" extendedLayoutApplyParent="Center">
			<JScrollPane extendedLayoutApplyParent="North">
				<JPanel>
					<SwappableCollection extendedArrayProcessingPath=".\ApplicationBuilder\Roku Channels:.url; .\ApplicationBuilder\Youtube Channels:.url">
						<JLabel setName="title" setForeground="100,50,150" setBackground="190,190,190">
						</JLabel>
						<JButtonArray setName="channels">
						</JButtonArray>
						<JButton setText="Close Video">
						</JButton>
					</SwappableCollection>
				</JPanel>
			</JScrollPane>
		</JPanel>
		<JButton setText="&lt;" extendedLayoutApplyParent="West" addActionListener="NavigationButtonActionListener" extendedActionListenerSubType="WidgetExtensions.Direction" extendedActionListenerSubTypeAttribute="BACKWARD">
		</JButton>
		<JButton setText="&gt;" extendedLayoutApplyParent="East" addActionListener="NavigationButtonActionListener" extendedActionListenerSubType="WidgetExtensions.Direction" extendedActionListenerSubTypeAttribute="FORWARD">
		</JButton>
		
		<MenuOption setText="File">
			<JMenuItem setText="Reload" addActionListener="ReloadActionListener">
			</JMenuItem >
			<JMenuItem setText="MinimizeToSystemTray" addActionListener="MinimizeActionListener">
			</JMenuItem >
			<JMenuItem setText="Exit" addActionListener="ExitActionListener">
			</JMenuItem >
		</MenuOption>
		
		<SystemTray setText="Video Launcher">
			<JMenuItem setText="Open">
			</JMenuItem>
			<JMenuItem setText="Close">
			</JMenuItem>
		</SystemTray>
	</JFrame>
</Build>
