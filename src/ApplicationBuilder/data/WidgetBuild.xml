<ButtonBuild>
	<Frame setLayout="BorderLayout" setText="Video Launcher" setSize="600, 250" setLocation="150,150">
		<Panel setLayout="BorderLayout" lay="CENTER">
			<ScrollPane lay="NORTH">
				<Panel>
					<Collection ArrayProcessingExt="" Path="">
						<Label setName="title" setForeground="100,50,150" setBackground="190,190,190">
						</Label>
						<ButtonArray setName="channels">
						</ButtonArray>
						<Button setText="Close Video">
						</Button>
					</Collection>
				</Panel>
			</ScrollPane>
		</Panel>
		<Button setText="&lt;" setLayoutApply="WEST" setActionListener="NavigationButtonActionListener" setActionListenerSubType="Direction.BACKWARD">
		</Button>
		<Button setText="&gt;" setLayoutApply="EAST" setActionListener="NavigationButtonActionListener" setActionListenerSubType="Direction.FORWARD">
		</Button>
		
		<MenuOption setText="File">
			<MenuItem setText="Reload" setActionListener="ReloadActionListener">
			</MenuItem >
			<MenuItem setText="MinimizeToSystemTray" setActionListener="MinimizeActionListener">
			</MenuItem >
			<MenuItem setText="Exit" setActionListener="ExitActionListener">
			</MenuItem >
		</MenuOption>
		
		<SystemTray setText="Video Launcher">
			<MenuItem setText="Open">
			</MenuItem>
			<MenuItem setText="Close">
			</MenuItem>
		</SystemTray>
	</Frame>
</ButtonBuild>
