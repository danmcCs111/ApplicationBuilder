<Build>	
	<JFrame setLayout="BorderLayout" setTitle="Video Launcher" setSize="750, 550" setLocation="150,150" setDefaultCloseOperation="3">
	
		<JPanel setLayout="BorderLayout" extendedLayoutApplyParent="Center">
			<JScrollPane extendedLayoutApplyParent="North">
				
					<SwappableCollection setName="swapVideos" setLayout="BorderLayout" extendedSetViewportView="" 
					extendedArrayProcessingPath=".\src\ApplicationBuilder\Roku Channels@.url; .\src\ApplicationBuilder\Youtube Channels@.url">
						<JLabel setText="TODO" setName="title" setForeground="100,50,150" setBackground="150,150,150" setHorizontalAlignment="0" extendedLayoutApplyParent="North">
						</JLabel>
						<JButtonArray setName="channels" setLayout="GridLayout" setHighlightBackgroundColor="240,240,240" setHighlightForegroundColor="50,50,50"
							extendedActionListenerArray="LaunchActionListener" extendedLayoutApplyParent="Center" setArrayForegroundAndBackground="240,240,240, 175,204,175"
							extendedSwappableHolder="swapVideos" >
						</JButtonArray>
						<JButton setText=" X Close Video" setName="closeLaunchAction" extendedLayoutApplyParent="South" addActionListener="LaunchActionListener" setBackground="175, 204, 175" setForeground="119,5,6">
						</JButton>
					</SwappableCollection>
					
			</JScrollPane>		
		</JPanel>
		
		<JPanel setLayout="BorderLayout" extendedLayoutApplyParent="West">
		<JButton setText="&lt;" extendedLayoutApplyParent="North" addActionListener="NavigationButtonActionListener" 
			extendedActionListenerSubType="WidgetExtensions.Direction@BACKWARD" extendedActionListenerConnectedComponent="WidgetExtensions.SwappableCollection">
		</JButton>
		</JPanel>
		
		<JPanel setLayout="BorderLayout" extendedLayoutApplyParent="East">
		<JButton setText="&gt;" extendedLayoutApplyParent="North" addActionListener="NavigationButtonActionListener" 
			extendedActionListenerSubType="WidgetExtensions.Direction@FORWARD" extendedActionListenerConnectedComponent="WidgetExtensions.SwappableCollection">
		</JButton>
		</JPanel>
		
		<JMenuBar extendedSetJMenuBarParent="">
			<JMenu setText="File" extendedLayoutApplyParent="">
				<JMenuItem setText="Reload" extendedLayoutApplyParent="" addActionListener="ReloadActionListener">
				</JMenuItem >
				<JMenuItem setText="MinimizeToSystemTray" extendedLayoutApplyParent="" addActionListener="MinimizeActionListener">
				</JMenuItem >
				<JMenuItem setText="Exit" extendedLayoutApplyParent="" addActionListener="ExitActionListener">
				</JMenuItem >
			</JMenu>
		</JMenuBar>
		
		<SystemTray setText="Video Launcher">
			<JMenuItem setText="Open" >
			</JMenuItem>
			<JMenuItem setText="Close" >
			</JMenuItem>
		</SystemTray>
		
	</JFrame>
</Build>
